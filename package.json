{
    "name": "cpp-copy-constructor-generator",
    "displayName": "C++ Copy Constructor Generator",
    "description": "Adds copy constructor and assignment operator to C++ classes.",
    "version": "0.0.1",
    "publisher": "yourpublishername",
    "engines": {
      "vscode": "^1.79.0"
    },
    "categories": [
      "Refactoring"
    ],
    "activationEvents": [
      "onCommand:cpp-copy-constructor-generator.generateCopyMethods"
    ],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "cpp-copy-constructor-generator.generateCopyMethods",
          "title": "C++ Copy Constructor Generator: Generate Copy Methods"
        }
      ],
      "menus": {
        "editor/context": [
          {
            "command": "cpp-copy-constructor-generator.generateCopyMethods",
            "group": "navigation",
            "when": "editorLangId == cpp"
          }
        ]
      }
    },
     "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
      "@types/vscode": "^1.79.0",
      "@types/mocha": "^10.0.1",
      "@types/node": "20.2.5",
      "@typescript-eslint/eslint-plugin": "^5.59.8",
      "@typescript-eslint/parser": "^5.59.8",
      "eslint": "^8.41.0",
      "glob": "^8.1.0",
      "mocha": "^10.2.0",
      "typescript": "^5.1.3",
      "@vscode/test-electron": "^2.3.2"
    }
  }